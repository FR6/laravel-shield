{
    "name": "vinkla/shield",
    "description": "A HTTP basic auth middleware for Laravel",
    "keywords": [
        "laravel",
        "http",
        "basic",
        "auth",
        "middleware",
        "shield"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Vincent Klaiber",
            "email": "hello@doubledip.se",
            "homepage": "https://doubledip.se"
        }
    ],
    "require": {
        "php": "^7.2",
        "illuminate/support": "^6.0",
        "symfony/http-kernel": "^4.3"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.16",
        "graham-campbell/analyzer": "^2.3",
        "graham-campbell/testbench": "^5.3",
        "phpunit/phpunit": "^8.0"
    },
    "config": {
        "preferred-install": "dist"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "3.0-dev"
        },
        "laravel": {
            "providers": [
                "Vinkla\\Shield\\ShieldServiceProvider"
            ]
        }
    },
    "autoload": {
        "psr-4": {
            "Vinkla\\Shield\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Vinkla\\Tests\\Shield\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
